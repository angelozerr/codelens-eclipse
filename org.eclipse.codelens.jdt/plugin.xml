<plugin>
	 
	<!-- Java "references" CodeLens -->
	<extension
      point="org.eclipse.codelens.codeLensProviders">
	   <codeLensProvider
	   		 name="%JavaReferencesCodeLensProvider.name"
	         class="org.eclipse.codelens.jdt.internal.JavaReferencesCodeLensProvider"
	         target="java.codelens">
	   </codeLensProvider>
	</extension>
	
	<!-- Java "implementations" CodeLens -->
	<extension
      point="org.eclipse.codelens.codeLensProviders">
	   <codeLensProvider
	   		 name="%JavaImplementationsCodeLensProvider.name"
	         class="org.eclipse.codelens.jdt.internal.JavaImplementationsCodeLensProvider"
	         target="java.codelens">
	   </codeLensProvider>
	</extension>	 
	 
</plugin>        
